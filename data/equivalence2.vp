attacker [passive]
principal A [
  knows public a
  knows private b
  knows public c
  joined_a = CONCAT(a,b,c)
]
principal B []
A -> B: joined_a
principal B [
  // TODO this is valid, but we currently can't parse it:
  b_a, b_b, b_c = SPLIT(joined_a)
]
queries [
  equivalence? a, b_a
  equivalence? b, b_b
  equivalence? c, b_c
]